/**index.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.container .bg {
  width: 100%;
  height: 692rpx;
}

.container .mode-list {
  flex: 1;
}

 .container .mode-item {
   width: 480rpx;
   height: 108rpx;
   line-height: 108rpx;
   margin-bottom: 40rpx;
   border-radius: 200rpx;
   text-align: center;
   color: #fff;
 }

 .mode-single,
 .mode-pk {
   background-color: #090909;
 }

 .mode-rank {
   background-color: #997A56;
 }

 .attendance-modal {
  position: fixed;
  z-index: 10;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 1;   
  background-color: #000;
  box-shadow: 0 0 200px 150px #ccc inset; 
  fill: blur(50px);
 }

 .attendance-body {
  width: 650rpx;
  height: 538rpx;
  display: flex;
  position: absolute;
  left: 50%;
  top: 320rpx;
  align-items: center;
  transform: translateX(-50%);  
  background-color: #ffffff;
  border-radius: 28rpx;
 }

.attendance-title {
  width: 400rpx;
  height: 74rpx;
  color: #997A56;
  font-size: 52rpx;
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.attendance-tip {
  width: 200rpx;
  height: 44rpx;
  color: #000000;
  font-size: 28rpx;
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.icon-wrapper {
  width: 650rpx;
  height: 80rpx;
  margin-top: 30rpx;
  display: flex;
}

.attendance-icon {
  width: 80rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 10rpx;
  background-size: 100% 100%;
}

.success {
  color: #997A56;
  background-image: url("data:image/png;base64,")
}

.fail {
  color: #e6e6e6;
  background-image: url("data:image/png;base64,")
}

.score-wrapper {
  width: 650rpx;
  height: 34rpx;
  margin-top: 30rpx;
  display: flex;
}

.attendance-score {
  width: 80rpx;
  height: 34rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 10rpx;
  background-size: 100% 100%;
  font-size: 26rpx;
}

.score-success {
  color: #997A56;
}

.score-fail {
  color:  #e6e6e6;
}

.take-attendance {
  width: 344rpx;
  height: 102rpx;
  color: #ffffff;
  background-color: #997A56;
  border-radius: 60rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 10;
  box-shadow: 0 -2rpx 2rpx 1rpx #ccc inset; 
}

.log {
  width: 650rpx;
  height: 30rpx;
  font-size: 30rpx;
  color: #000000;
}
  