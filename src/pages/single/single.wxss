/* pages/single/single.wxss */
.container {
  padding: 40rpx 30rpx;
}

.container .idiom-item-wrapper {
  float: left;
  width: 16.67%;
  text-align: center;
  margin: 12rpx 0;
}

.container .idiom-item {
  width: 104rpx;
  height: 124rpx;
  line-height: 124rpx;
  display: inline-block;
  font-size: 60rpx;
  text-align: center;
  background-size: 100% 100%;
}

.idiom-item.passed {
  color: #997A56;  
  background-image: url("data:image/png;base64,")
}

.idiom-item.current {
  color: #F5DBBC;
  background-image: url("data:image/png;base64,")
}

.idiom-item.disabled {
  color: #F1EFF0;
  background-image: url("data:image/png;base64,")
}