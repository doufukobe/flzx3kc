.bg {
  position: fixed;
  z-index: 0;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.over-icon {
  width: 362rpx;
  height: 262rpx;
  position: absolute;
  left: 50%;
  top: 200rpx;
  transform: translateX(-50%)
}

.content-wrapper {
  width: 540rpx;
  height: 500rpx;
  display: flex;
  background-color: #ffffff;
  border-radius: 24rpx;
  position: absolute;
  align-items: center;
  left: 50%;
  top: 320rpx;
  transform: translateX(-50%)
}

.over-title {
  width: 200;
  height: 66rpx;
  color: #090909;
  font-size: 48rpx;
  margin-top: 120rpx;
}

.action-btn {
  width: 320rpx;
  height: 80rpx;
  border-radius: 200rpx;
  color: #ffffff;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.share {
  background-color: #997A56;
  margin-top: 50rpx;  
}

.go-back {
  background-color: #000000; 
  margin-top: 40rpx;
}