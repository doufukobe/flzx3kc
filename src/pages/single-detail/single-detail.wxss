/* pages/single-detail/single-detail.wxss */

@import "../../common/character.wxss";

.img-wrapper {
  width: 100%;
  height: 570rpx;
  position: relative;
  text-align: center;
}

.img-wrapper .level-prompt {
  position: absolute;
  left: 50%;
  top: 0;
  transform: translateX(-50%);
  font-size: 32rpx;
  color: #fff;
  width: 240rpx;
  height: 40rpx;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,")
}

.img-wrapper .bg-img {
  width: 100%;
  height: 100%;
}

.img-wrapper .idiom-img {
  position: absolute;
  width: 480rpx;
  height: 320rpx;
  top: 50%;
  left: 50%;
  /* 模拟器有 bug，使用 translate 不行 */
  transform: translateX(-50%) translateY(-50%);
}

/* 按钮部分 */
.operation-wrapper {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0 60rpx;
}

.operation-wrapper .operation-item {
  width: 260rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  text-align: center;
  color: #fff;
  border-radius: 200rpx;
  margin: 0 30rpx;
}

.operation-coin {
  width: 160rpx;
  height: 50rpx;
  display: flex;
  float: right;
  align-items: center;
  justify-content: center;
  margin-bottom: 30rpx;
  margin-right: 30rpx;
}

.coin {
  width: 44rpx;
  height: 44rpx;
}

.coin-num {
  width: 60rpx;
  height: 44rpx;
  color: #997A56;
  margin-left: 10rpx
}

.operation-wrapper .operation-prompt {
  background-color: #000000;
}

.operation-wrapper .operation-help {
  background-color: #997A56;
}

.operation-wrapper .operation-skip {
  background-color: #997A56;
}

/* 输入成语部分 */
.idiom-input-fields {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 60rpx 144rpx;
  position: relative;
  z-index: 10;
}

.idiom-input-fields .idiom-character {
  width: 108rpx;
  height: 108rpx;
  line-height: 108rpx;
  text-align: center;
  font-size: 56rpx;
  color: #997A56;
  border: 2rpx solid #997A56;
  box-sizing: border-box;
}

/* 吸底图片 */
.fixed-download-bg {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 500rpx;
  z-index: 0;
}

/* 成功模态框 */
.success-modal {
  position: fixed;
  z-index: 10;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  opacity: 1;   
  background-color: #000;
  box-shadow: 0 0 200px 150px #ccc inset; 
  fill: blur(50px);
}

.success-modal .modal-body {
  width: 540rpx;
  text-align: center;
  border-radius: 24rpx;
  background-color: #fff;
}

.success-modal .modal-icon {
  width: 320rpx;
  height: 212rpx;
  margin: -106rpx auto 0;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}

.success-modal .modal-title {
  font-size: 48rpx;
  color: #997A56;
  letter-spacing: 0;
  text-align: center;
  margin: 8rpx auto 0;
}

.success-modal .modal-content {
  opacity: 0.4;
  font-size: 28rpx;
  color: #090909;
  letter-spacing: 0;
  text-align: center;
  line-height: 44rpx;
  margin: 16rpx 40rpx 30rpx;
}

.success-modal .modal-award {
  font-size: 32rpx;
  color: #997A56;
  text-align: center;
  margin: 30rpx auto 32rpx;
}

.success-modal .modal-button {
  width: 320rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 32rpx auto 40rpx;
  background: #090909;
  border-radius: 200rpx;
  color: #fff;
}

/* toast 提示 */
.toast {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 320rpx;
  height: 240rpx;
  text-align: center;
  background: rgba(0,0,0,0.80);
  border-radius: 24rpx;
  color: #fff;
  /* display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around; */
}

.toast .toast-icon {
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
  width: 116rpx;
  height: 100rpx;
  margin: 34rpx auto 22rpx;
  position: relative;
  left: 10rpx;
}